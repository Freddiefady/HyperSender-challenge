{
    "tabWidth": 4,
    "semi": true
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "arrowParens": "always",
    "plugins": ["./node_modules/prettier-plugin-blade/", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": ["*.php", "*.blade.php"],
            "vscodeLanguageIds": ["blade", "php"],
            "options": {
                "parser": ["php", "blade"]
            }
        }
    ]
}
